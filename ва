z = input("Введите учебние день:  ")
if (z == "Понедилок"):
      class Робочи_дни():
         def __init__(self, номер_ур, урок, час, оцынка):
            self.номер_ур = номер_ур
            self.урок = урок
            self.час = час
            self.оцынка = оцынка
      class Pon():
              Урок_1 = Робочи_дни(1, 'Math', '08:00', 8)
