              Урок_8 = Робочи_дни(8, 'Зарубыжна.лыт', '12:45', 9)
              print("Номер     Урок     Вскыльки     Бали")
              print(str(Урок_1.номер_ур) + "  " +Урок_1.урок  + "  " + Урок_1.час  + "  " + str(Урок_1.оцынка))
              print(str(Урок_2.номер_ур) + "  " +Урок_2.урок  + "  " + Урок_2.час  + "  " + str(Урок_2.оцынка))
              print(str(Урок_3.номер_ур) + "  " +Урок_3.урок  + "  " + Урок_3.час  + "  " + str(Урок_3.оцынка))
