if (z == "Четверг"):
       class Робочи_дни():
           def __init__(self, номер_ур, урок, час, оцынка):
            self.номер_ур = номер_ур
            self.урок = урок
            self.час = час
            self.оцынка = оцынка
       class fer(Робочи_дни):
          def __init__(self, номер_ур, урок, час, оцынка):
             super().__init__(номер_ур, урок, час,оцынка)
       class Cha():
           Урок_1 = Робочи_дни(1, 'Зарубыжна', '08:45', 11)
